{
  "env": {
    "browser": true
  },
  "root": true,
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": ["error"],
    "semi": [
      "error",
      "never"
    ]
  }
}